		 * The settings for this page.
		 */
		public static const SETTINGS:String = "settings";
		
		public static const STATICRESOURCES:String = "staticresources";

		public static const STATUSES:String = "statuses";

		public static const SUBSCRIPTIONS:String = "subscriptions";
		
		public static const SUBSCRIBEDTO:String = "subscribedto";
		
		public static const SUBSCRIBERS:String = "subscribers";

		/**
		 * The Page's profile tabs.
		 */
		public static const TABS:String = "tabs";
		
		/**
		 * The photos, videos, and posts in which the object has been tagged.
		 */
		public static const TAGGED:String = "tagged";

		/**
		 * The Users tagged in the photo.
		 */
		public static const TAGS:String = "tags";
		
		public static const TELEVISION:String = "television";
		
		public static const TRANSLATIONS:String = "translations";
		
		public static const UPDATES:String = "updates";
		
		public static const VIDEOS:String = "videos";
