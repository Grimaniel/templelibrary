		 * All of the users who have been invited to this event.
		 */
		public static const INVITED:String = "invited";
		
		public static const LIKES:String = "likes";
		
		public static const LINKS:String = "links";

		public static const LOCATIONS:String = "locations";

		/**
		 * All of the users who have been responded "Maybe" to their invitation to this event
		 */
		public static const MAYBE:String = "maybe";
		
		/**
		 * All of the users who are members of this list.
		 */
		public static const MEMBERS:String = "members";
		
