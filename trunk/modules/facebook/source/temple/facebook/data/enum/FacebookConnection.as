package temple.facebook.data.enum
{
	/**
	 * Enumerator class containing the possible values of a Facebook connection.
	 * 
	 * @author Thijs Broerse
	 */
	public final class FacebookConnection
	{
		/**
		 * The Facebook apps and pages owned by the current user.
		 */
		public static const ACCOUNTS:String = "accounts";
		
		public static const ADACCOUNTS:String = "adaccounts";

		/**
		 * A list of the Page's Admins.
		 */
		public static const ADMINS:String = "admins";
		
		public static const ACHIEVEMENTS:String = "achievements";
		
		public static const ACTIVITIES:String = "activities";
		
		public static const ALBUMS:String = "albums";
		
		public static const APPREQUESTS:String = "apprequests";

		/**
		 * All of the users who are attending this event.
		 */
		public static const ATTENDING:String = "attending";

		public static const BANNED:String = "banned";
		
		/**
		 * A list of users blocked from the Page.
		 */
		public static const BLOCKED:String = "blocked";

		public static const BOOKS:String = "books";
		
		public static const CHECKINS:String = "checkins";

		public static const COMMENTS:String = "comments";

		/**
		 * All of the users who declined their invitation to this event
		 */
		public static const DECLINED:String = "declined";

		public static const DOCS:String = "docs";
		
		public static const EVENTS:String = "events";

		public static const FAMILY:String = "family";

